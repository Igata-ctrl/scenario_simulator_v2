      <memberdef kind="variable" id="is__console_8hpp_1aecb6dfc9579a3bb52429f2e76bf181ae" prot="public" static="no" mutable="no">
        <type>auto</type>
        <definition>auto openscenario_interpreter::console::is_console</definition>
        <argsstring></argsstring>
        <name>is_console</name>
        <initializer>= [](const auto &amp; os) {
  if (os.rdbuf() == std::cout.rdbuf()) {
    static const auto result{static_cast&lt;bool&gt;(::isatty(STDOUT_FILENO))};
    return result;
  } else if (os.rdbuf() == std::cerr.rdbuf()) {
    static const auto result{static_cast&lt;bool&gt;(::isatty(STDERR_FILENO))};
    return result;
  } else {
    return false;
  }
}</initializer>
